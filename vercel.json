{
  "version": 2,
  "installCommand": "npm install && npm install --save-dev tsx esbuild typescript @vercel/node",
  "buildCommand": "npm run build && mkdir -p dist/client && mkdir -p dist/server && ([ -d \"dist/assets\" ] && cp -r dist/assets dist/client/ || true) && ([ -f \"dist/index.html\" ] && cp dist/index.html dist/client/ || true)",
  "outputDirectory": "dist",
  "framework": "vite",
  "public": true,
  "builds": [
    {
      "src": "vercel-start.js",
      "use": "@vercel/node"
    },
    {
      "src": "dist/client/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/dist/client/assets/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/dist/client/favicon.ico"
    },
    {
      "src": "/api/(.*)",
      "dest": "/vercel-start.js"
    },
    {
      "src": "/ws",
      "dest": "/vercel-start.js",
      "headers": {
        "Connection": "upgrade",
        "Upgrade": "websocket"
      }
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/vercel-start.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "VERCEL": "1"
  },
  "functions": {
    "vercel-start.js": {
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}